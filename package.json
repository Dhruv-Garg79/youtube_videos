{
	"name": "fampay_yt_be",
	"version": "1.0.0",
	"description": "fampay youtube video APIs",
	"main": "lib/index.js",
	"author": "Dhruv Garg",
	"license": "MIT",
	"scripts": {
		"lint": "eslint --fix 'src/**/*.{js,ts}'",
		"format": "prettier --write src/**",
		"test": "jest --rootDir=tests",
		"compile": "tsc --build",
		"compile:watch": "tsc --build --watch",
		"dev": "NODE_ENV=dev nodemon --experimental-modules --es-module-specifier-resolution=node",
		"build:dev": "concurrently --kill-others \"yarn compile\" && NODE_ENV=dev nodemon --experimental-modules --es-module-specifier-resolution=node"
	},
	"engines": {
		"node": "14"
	},
	"dependencies": {
		"axios": "^1.2.2",
		"compression": "^1.7.4",
		"cron": "^2.1.0",
		"express": "^4.18.2",
		"helmet": "^6.0.1",
		"mongodb": "^4.13.0",
		"morgan": "^1.10.0",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@babel/core": "^7.17.5",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/plugin-proposal-class-properties": "^7.16.7",
		"@babel/plugin-proposal-object-rest-spread": "^7.17.3",
		"@babel/plugin-transform-modules-commonjs": "^7.17.9",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@babel/runtime": "^7.17.2",
		"@types/cron": "^2.0.0",
		"@types/express": "^4.17.15",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.15.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.2.3",
		"babel-plugin-source-map-support": "^2.1.3",
		"concurrently": "^7.6.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.10.0",
		"eslint-plugin-promise": "^6.0.0",
		"husky": "^4.3.8",
		"lint-staged": "^12.3.4",
		"nodemon": "^2.0.20",
		"prettier": "^2.5.1",
		"typescript": "4.5.5"
	},
	"lint-staged": {
		"src/**/*.{js,ts}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "tsc"
		}
	}
}
